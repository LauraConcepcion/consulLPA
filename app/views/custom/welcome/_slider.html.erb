  <div class="orbit-wrapper">
    <ul class="orbit-container no-bullet" tabindex="0">
      <li class="orbit-slide" aria-live="polite">
        <a href="/budgets" />
          <figure class="orbit-figure">
            <%= image_tag("Banner-Presupuestos-2022.jpg", class: "orbit-image", alt: "Presupuestos Participativos 2022") %>
          </figure>
        </a>
      </li>
    </ul>
  </div>
</div>
